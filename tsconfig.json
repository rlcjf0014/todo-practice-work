{
    "compileOnSave": true,
    "compilerOptions": {
      "declaration": false,
      "experimentalDecorators": true,
      "emitDecoratorMetadata": true,
      "module": "commonjs",
      "newLine": "LF",
      "noFallthroughCasesInSwitch": true,
      "noImplicitAny": true,
      "noImplicitReturns": true,
      "noImplicitThis": false,
      "noUnusedParameters": false,
      "noUnusedLocals": true,
      "outDir": "dist",
      "skipLibCheck": true,
      "sourceMap": false,
      "strictNullChecks": false,
      "allowSyntheticDefaultImports": true,
      "target": "es2016"
    },
    "include": [
      "fail/**/*.ts", "index.ts", "lib/api-server.ts"
    ]
  }
  